#include <stdio.h>

int main()
{
    int i,n,x,sum=0;
    

    printf("\nEnter how many values you want to enter : ");
    scanf("%d",&n);

    printf("\nEnter integer value to find percentage : \n");

    for(i=0;i<n;i++)
    {
          scanf("%d",&x);
          sum=sum+x;
    }
    printf("\nPercentage = %.2f",sum/(float)n);

return 0; 
}
