#include <stdio.h>

int main()
{
    int a,b;
    printf("\nEnter value of a = ");
    scanf("%d",&a);
    printf("Enter value of b = ");
    scanf("%d",&b);


    printf("\n& Bitwise operator result -> %d",(a&b));
    printf("\n| Bitwise operator result -> %d",(a|b));
    printf("\n^ Bitwise operator result -> %d",(a^b));

    return 0;
}
