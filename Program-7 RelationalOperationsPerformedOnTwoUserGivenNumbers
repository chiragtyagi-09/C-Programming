#include <stdio.h>
int main()
{
    int a,b;
    printf("\nEnter value of a = ");
    scanf("%d",&a);
    printf("Enter value of b = ");
    scanf("%d",&b);
    
    printf("\n== Relational operator result -> %d\n",a==b);
    printf("!= Relational operator result -> %d\n",a!=b);
    printf("> Relational operator result -> %d\n",a>b);
    printf("< Relational operator result -> %d\n",a<b);
    printf(">= Relational operator result -> %d\n",a>=b);
    printf("<= Relational operator result -> %d\n",a<=b);
    
    return 0;
}
