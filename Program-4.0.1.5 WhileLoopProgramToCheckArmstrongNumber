#include <stdio.h>

int main()
{
     int i=1,n,r,sum=0,temp;

     printf("\nEnter number to check whether it is Armstrong or not  : ");
     scanf("%d",&n);
     temp=n;

     while( n>0 )
     {
          r=n%10;    
          sum=sum+(r*r*r);    
          n=n/10;  
     }
     if (temp==sum)
     {
       printf("\nArmstrong number\n");
     }
     else
     {
       printf("\nNot an armstrong number\n");
     }


return 0;
}
